{
  "name": "server",
  "version": "1.0.0",
  "description": "serverside api",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node --unhandled-rejections=strict ./build/app.js",
    "debug": "export DEBUG=* && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --project ./"
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "inlineSourceMap": true
  },
  "keywords": [
    "server",
    "api"
  ],
  "author": "Nathi",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "winston": "^3.3.3",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "source-map-support": "^0.5.16",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
